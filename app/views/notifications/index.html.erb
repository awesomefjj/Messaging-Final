<p id="notice"><%= notice %></p>

<h1>Notifications</h1>

<table>
  <thead>
    <tr>
      <th>Tenant</th>
      <th>Tenant name</th>
      <th>Phone num</th>
      <th>Title</th>
      <th>Content</th>
      <th>Receive</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notifications.each do |notification| %>
      <tr>
        <td><%= notification.tenant_id %></td>
        <td><%= notification.tenant_name %></td>
        <td><%= notification.phone_num %></td>
        <td><%= notification.title %></td>
        <td><%= notification.content %></td>
        <td><%= notification.receive_id %></td>
        <td><%= notification.create_t %></td>
        <td><%= link_to 'Show', notification %></td>
        <td><%= link_to 'Edit', edit_notification_path(notification) %></td>
        <td><%= link_to 'Destroy', notification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Notification', new_notification_path %>
