<h1>Dashboard</h1>
<%= Gon::Base.render_data %>

<div class="list-group">
   
        <%= render 'informations' %>
            
</div>
<div class="container d-flex justify-content-between pd-0">
    <div class="card col-8 mr-4 shadow">
        <div class="card-title">消息量统计</div>
        <div>
            <%= render 'charts' %>
        </div>
    </div>
    <div class="card col-4 shadow">
        <div class="card-body">
            <span class="d-block font-14 font-weight-500 text-dark">总发送量</span>
            <div class="display-3 font-weight-500 text-dark"><%= @notifications_all.count %></div>
        </div>
    </div>
</div>
<div class="d-flex flex-row justify-content-around pt-5">
    <%= render 'number' %>
</div>

<div class="container mt-5" sytle="height: 200px;">
    <div class="d-flex align-items-end justify-content-center ">
        Designed and Developed by Jeffery.
    </div>
</div>
<div class="chart-area">
<canvas id="myAreaChart"></canvas>
</div>
