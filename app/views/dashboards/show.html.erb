<h1>Dashboard</h1>
<%= Gon::Base.render_data %>

<div class="list-group">
   
        <%= render 'informations' %>
            
</div>
<div class="container d-flex justify-content-between pt-5" style="padding:0">
    <div class="card col-8 mr-1 shadow" style="padding:0;  background-color: #fff6d4a1">
        <strong class="card-header text-white" style="background-color: #36bea6">消息量统计</strong>
        <div class="card-body">
            <%= render "charts" %>
        </div>
    </div>
    <div class="card col-3 shadow" style="padding:0; background-color: #fff6d4a1">
        <strong class="d-block card-header font-14 font-weight-500 text-white" style="background-color:#36bea6">
            总发送量
        </strong>
        <div class="card-body">
            <div class="display-3 font-weight-500 text-dark"><%= @notifications_all.count %></div>
        </div>
    </div>
</div>
<div class="d-flex flex-row justify-content-around pt-5">
    <%= render 'number' %>
</div>

<div class="container mt-5" sytle="height: 200px;">
    <div class="d-flex align-items-end justify-content-center ">
        Designed and Developed by Jeffery.
    </div>
</div>



