
    <title>CanvasJS Chart jQuery Plugin</title>
    <script type="text/javascript" src="https://canvasjs.com/assets/script/jquery-1.11.1.min.js"></script>
    <script type="text/javascript" src="https://canvasjs.com/assets/script/jquery.canvasjs.min.js"></script>
    <%= Gon::Base.render_data %>
    <script type="text/javascript">
        
        var xx = gon.noti_num;
        $(function () {
            $("#chartContainer").CanvasJSChart({ //Pass chart options

                axisX: {
                    valueFormatString: "MMM"
                },
                axisY: {
                    prefix: ""
                },
                data: [
                    {
                        yValueFormatString: "### Êù°",
                        type: "splineArea", //change it to column, spline, line, pie, etc
                        dataPoints: [
                            { x: new Date(2021,0), y: 230 },
                            { x: new Date(2021,1), y: 156 },
                            { x: new Date(2021,2), y: xx  },
                            { x: new Date(2021,3), y: 356 },
                            { x: new Date(2021,4), y: 452 },
                            { x: new Date(2021,5), y: 156 },
                            { x: new Date(2021,6), y: 333 },
                            { x: new Date(2021,7), y: 256 },
                            { x: new Date(2021,8), y: 224 },
                            { x: new Date(2021,9), y: 298 },
                            { x: new Date(2021,10), y: 230 },
                            { x: new Date(2021,11), y: 366 }
                    
                        ]
                    }
                ]
            });

        });
    </script>


<div id="chartContainer" style="width:100%; height:300px;"></div>